        var widget = jQuery('#click_withinLazyLoad_tower'); 
        if (!widget.size()) {
          return;
        } 
        
        var cw = {};
          amznJQ.available('search-js-general', function() {
            amznJQ.available('search-js-autocomplete', function() {
              SPUtils.afterEvent('searchajax', function() {    
                


window.pa_cw_tower = 1;   
window.pa_asins_tower;
window.pa_sa_tower;
window.pa_bl_tower;
window.pa_kw_tower;
var browseNodeMap = {};

