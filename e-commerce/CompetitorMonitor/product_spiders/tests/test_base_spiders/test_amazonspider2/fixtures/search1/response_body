browseNodeMap.n908798031 = "appliancesBN";

var lazyLoadURL = '/gp/product-ads/lazyLoad/handler/click-within-handler.html';

                

  var generateAjaxCall = function(keyword, searchAlias, browseNode, seenAsins, 
    isBrowse, paDebug, pageType, subPageType, placement, useAui, bl, childBrowseNodes) {
    var lazyLoadURL = '/gp/product-ads/lazyLoad/handler/click-within-handler.html';

    jQuery.ajax({
      
      url: lazyLoadURL + "?searchTerms="+encodeURIComponent(keyword)+
                         "&searchAlias="+encodeURIComponent(searchAlias)+
                         "&isDebug=" + paDebug + "&widgetName="+encodeURIComponent(widgetName)+
                         "&pageType=" + pageType + "&subPageType=" + subPageType +
                         "&browseLadder="+encodeURIComponent(bl)+
                         "&childBrowseNodes="+encodeURIComponent(childBrowseNodes)+
                         "&placement=" + placement + "&seenasins="+encodeURIComponent(seenAsins)+
                         "&useAui=" + useAui +
                         "&suggestedBN="+encodeURIComponent(browseNode)+"&isBrowse="+isBrowse,
      type: "GET",
      cache : false,
      success : function(html) { 
        jQuery('#click_withinLazyLoad_' + placement).html(html);    
      },
      error : function(html) { 
        jQuery('#click_withinLazyLoad_' + placement).html("<div></div>");    
      }
    }); 
  }


                
  var cw_extractParams = function(inputString) {
    var paramString = unescape(inputString); 
    var eParams = paramString.split(",");
    return eParams; 
  } 

  var regexRh =  /([?&=])(rh=)([^&]*)/;

  var shouldPunt = function(bl) {
    var cwurl = location.href;
    var regexField = /([?&=])(field-brandtextbin=)([^&]*)/;                  
    var regexSort = /([?&=])(sort=)([-\w]+)/; 
    var regexMe = /([?&=])(me=)([^&]*)/; 
    var regexScn = /([?&=])(scn=)([-\w]+)/; 
    var regexSrs = /(srs=)([^&]*)/;
    var regexSearchAlias = /(search-alias=)([^&]*)/;
    var hashindex = cwurl.indexOf("#"); 
    var rhstring;
    var sortstring;

    if (hashindex == -1) {
      rhstring = regexRh.exec(cwurl);
      sortstring = regexSort.exec(cwurl);
    } else {   
      hashstring = cwurl.substring(hashindex+1);
      rhstring = regexRh.exec(hashstring);
      sortstring = regexSort.exec(hashstring);
    }

    if (bl == null || bl.length == 0) {
      return true;             
    }

    if (rhstring != null && rhstring.length != 0) {
      var rhParams = cw_extractParams(rhstring);       
      var pat = /(\b[n|k|i]\b)/; 
      for (var x = 0; x < rhParams.length; x++) {
        var entry, rhKey;
        entry = rhParams[x].split(":"); 
        if (entry.length == 2) {
          rhKey = entry[0];                 
          if(!pat.test(rhKey)) {                          
            return true;
          }
        } 
      }
    }
    if (regexField.test(unescape(cwurl))) {                     
      return true;
    }
    if(regexMe.test(unescape(cwurl))) {                    
      return true;
    }
    if(sortstring != null && sortstring.length != 0) {
      var sortParams = cw_extractParams(sortstring);         
      for (var x = 0; x < sortParams.length; x++) { 
        var entry, sortVal;
        entry = sortParams[x].split("=");
        if (entry.length == 2) {
          sortVal = entry[1];
          if(sortVal != "relevancerank" && sortVal != "") {
            return true;
          }
        }  
      }
    }

    return false;
  }
                  

                
var getSeenAsins = function(keyword) {
  if (typeof atfAdAsinList !== 'undefined') {
     window.pa_asins_tower = atfAdAsinList;
  } else if((typeof(window.pa_bl_tower) != "undefined" && window.pa_bl_tower != bl) || 
    (typeof(window.pa_kw_tower) != "undefined" && window.pa_kw_tower != keyword)) {
    window.pa_asins_tower = undefined;
  } else if(typeof(asinList) != "undefined") {
    if(asinList == 0) {
    jQuery('#click_withinLazyLoad_tower').html("<div></div>");
    return;
  } else if (typeof(window.pa_asins_tower) != "undefined") { 
    window.pa_asins_tower = window.pa_asins_tower + asinList;                       
  } else {
    window.pa_asins_tower = asinList; 
  }
                    
  var temp = window.pa_asins_tower.split(",");
    if(temp.length > 40 ) {
      var extra = temp.length - 40; 
      for (var i = 0; i < extra; i++) {
        temp.shift();
      }
    } 
    window.pa_asins_tower = temp.join(); 
  }  
}



                var getNodeParameter = function(path, excludeHidden) {
                  var suggestedBnUrl = regexRh.exec(path);
                  var suggestedBnParams = cw_extractParams(suggestedBnUrl);
                  for (var i = 0; i < suggestedBnParams.length; i++) {
                    var sEntry;
                    sEntry = suggestedBnParams[i].split(":");
                    if (sEntry.length === 2 && sEntry[0] === 'n') {
                      if (!excludeHidden) {
                        return sEntry[1];
                      } else if (sEntry[1].length > 0 && sEntry[1][0] != '!') {
                        return sEntry[1];
                      }
                    }
                  }
                }

                var bl;
                if (jQuery.pageContext && jQuery.pageContext.browseLadder) {
                  bl = unescape(jQuery.pageContext.browseLadder);
                }    

                if (shouldPunt(bl)) {
                  jQuery('#click_withinLazyLoad_tower').html("<div></div>");
                  return;
                }
             
                var isBrowse = 0;

                var suggestedBN = [];
                if (bl === 'i:aps') {
                  var urlattr = jQuery("#refinements > ul > li > a:first").attr("href");  
                  suggestedBN.push(getNodeParameter(urlattr, false));                                       
                }

                var bnlist = bl.split(',');
                var bn = bnlist[0].replace(":","");          
                var kw = iss.keyword();
                var sa = SearchPageAccess.searchAlias();
                if (!kw) {
                  kw = jQuery("#refinements > ul > li > strong").html();
                  isBrowse = 1;
                }

                var childBrowseNodes = [];
                var currentCategory = jQuery("#refinements > ul > li > a > span.boldRefinementLink");

                if (!currentCategory || currentCategory.length == 0) {
                  currentCategory = jQuery("#refinements > ul > li > strong");
                }

                var childHref;
                if (currentCategory) {
                  childHref = currentCategory.parent().next().find("a>span").first().parent().attr('href');
                }

                if (!isBrowse && childHref) {
                  childBrowseNodes.push(getNodeParameter(childHref, true));
                }

                getSeenAsins(kw);

                window.pa_bl_tower = bl; 
                window.pa_kw_tower = kw;
                window.pa_sa_tower = sa;

                

                if (!(browseNodeMap.hasOwnProperty(bn) || bl === 'i:aps') || !kw)                        
                {               
                  jQuery('#click_withinLazyLoad_tower').html("<div></div>"); 
                  return;
                }

                generateAjaxCall(kw, sa, suggestedBN, window.pa_asins_tower, isBrowse, '0', 
                  'Search', 'portal-batch-slow-btf', 'tower', '0', bl, childBrowseNodes);
              });
          });
        });
      });

  

  }
  </script>














 
 
 


