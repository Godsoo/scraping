    var imageSrc = "http://ecx.images-amazon.com/images/I/51eu9GG46DL._SX342_.jpg";
    var colorImages = {"initial":[{"large":"http://ecx.images-amazon.com/images/I/51eu9GG46DL.jpg","landing":["http://ecx.images-amazon.com/images/I/51eu9GG46DL._SX342_.jpg"],"hiRes":"http://ecx.images-amazon.com/images/I/61PWVNaO55L._SL1001_.jpg","thumb":"http://ecx.images-amazon.com/images/I/51eu9GG46DL._SX38_SY50_CR,0,0,38,50_.jpg","main":["http://ecx.images-amazon.com/images/I/61PWVNaO55L._SX342_.jpg","http://ecx.images-amazon.com/images/I/61PWVNaO55L._SX385_.jpg","http://ecx.images-amazon.com/images/I/61PWVNaO55L._SX425_.jpg","http://ecx.images-amazon.com/images/I/61PWVNaO55L._SX466_.jpg","http://ecx.images-amazon.com/images/I/61PWVNaO55L._SX522_.jpg"]}]};

    (function(doc) {
        var mi = doc.getElementById("main-image");
        var w = window.innerWidth || doc.body.offsetWidth;
        var widths = [1280,1440,1640,1800];
        var sz = 0;
        for(;sz < 4 && w >= widths[sz];  sz++);
        if(sz || 1)  {
            var miw = doc.getElementById("main-image-widget");
            miw.className = miw.className.replace(/size[0-9]+/,"size"+sz);
                var newImageSize = [[342,"299"],[385,"336"],[425,"371"],[466,"407"],[522,"456"]];
                if(newImageSize[sz][0] >0 && newImageSize[sz][1] >0 ) {
                    mi.width  = newImageSize[sz][0];
                    mi.height = newImageSize[sz][1];
                }
             
            amznJQ.onCompletion("amznJQ.AboveTheFold", function() {
                    var src = colorImages.initial[0].main[sz];
                var img = new Image();
                img.onload = function() {
                    var clone = mi.cloneNode(true);
                    clone.src = src;
                    clone.removeAttribute("width");
                    clone.removeAttribute("height");
                    clone.removeAttribute("onload");
                    mi.parentNode.replaceChild(clone, mi);
                    mi = clone;
                    amznJQ.declareAvailable('ImageBlockATF');
                };
                img.src = src;
            });
           } else {
    	        amznJQ.declareAvailable('ImageBlockATF');
	   }
        mi.style.display = "inline";
    })(document);
</script>











