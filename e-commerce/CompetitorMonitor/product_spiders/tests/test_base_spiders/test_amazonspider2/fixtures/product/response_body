<script>
    amznJQ.onReady('jQuery', function() {
        jQuery.ajax({
            url: "/gp/gc/widget/product-gc/276-6101353-7700210?ie=UTF8&asin=B00FRLPAWK&productGroupID=gl_sports",
            type: 'GET',
            async: true,
            dataType: 'html',
            tryCount: 0,
            retryLimit: 3,
            success: function (data, status, request) {
                jQuery('#product-gc').html(data);
            },
            error: function (data, status, request) {
                   // simple retry mechanism;
                   if (this.tryCount++ < this.retryLimit) {
                       jQuery.ajax(this);
                   } else { }
                }
        });
    });
</script>

</div>
