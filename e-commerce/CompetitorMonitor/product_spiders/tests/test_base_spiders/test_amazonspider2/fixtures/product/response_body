  twisterController.init(data);
  
        window.SizingChart = function() {
            var isOpen = false;
            var open = function(link) {
                amznJQ.available("jQuery", function() {
                    var spinnerContent = "<div id='size-chart-spinner'><img src='"
                        + window.AmazonPopoverImages.snake + "'/></div>";
                    var url = jQuery(link).attr("href");
                    if (window.gOpenQuickview) {
                        window.gOpenQuickview.showSizingChart(url);
                    } else {
                        if(isOpen) return;
                        isOpen = true;
                        amznJQ.available("popover", function() {
                            var popover =  jQuery.AmazonPopover.displayPopover({
                                    literalContent: spinnerContent,
                                    location:"centered",
                                    width:800,
                                    zIndex:991,
                                    height:height,
                                    locationOffset:[0, -50],
                                    title:"Size Chart",
                                    closeText:"Close",
                                    onHide: function() {
                                        isOpen = false;
                                    }
                                });

                            var iframeHeight = jQuery(window).height() - 180;
                            var height = Math.min(Math.max(iframeHeight,300), 700);

                            var fetchAjax = function() {
                                jQuery.ajax({
                                    url:url+"&height="+height,
                                    success: function(data) {
                                        data = data.replace(/\s*([^\s]*)\s*$/, "$1");
                                        if(data) {
                                            popover.find(".ap_content").html(data);
                                        } else {
                                            ajaxFailure();
                                        }
                                    },
                                    error: function() {
                                        ajaxFailure();
                                    }
                                });
                            };

                            var ajaxFailure = function() {
                                popover.find(".ap_content").html("<div id='size-chart-error'><p class='error-text'>We're sorry, due to a temporary problem, this sizing chart cannot be loaded.</p><p><a href='" + url + "'>Try again</a></p></div>");

                                popover.find("#size-chart-error a").click(function() {
                                    popover.find(".ap_content").html(spinnerContent);
                                    fetchAjax();
                                    return false;
                                });
                            };

                            fetchAjax();
                        });
                    }
                });

                return false;
            };

            return { open : open };
        }();
    
try {
     jQuery('#size-chart-url').children().attr('href',"/gp/product/ajax-handlers/apparel-sizing-chart.html/ref=dp_sizechart_b?ie=UTF8&asin=B00FRLPAWK");
    } catch (e) {
    }
  window.jqupgrade =  0;
  amznJQ.declareAvailable('newTwister');
});
}); 
});

</script>





 








<script type="text/javascript">

amznJQ.onCompletion('amznJQ.criticalFeature', function() {
  amznJQ.available('search-js-jq', function(){});
  amznJQ.available('amazonShoveler', function(){});
  amznJQ.available('simsJS', function(){});
  amznJQ.available('cmuAnnotations', function(){});
  amznJQ.available('externalJS.tagging', function(){});
  amznJQ.available('amzn-ratings-bar', function(){});
  amznJQ.available('accessoriesJS', function(){});
  amznJQ.available('priceformatterJS', function(){});
  amznJQ.available('CustomerPopover', function(){});

if (typeof(loadState) != "undefined" && !loadState) {

(function(){
    if(window.ue && typeof ue.tag === 'function'){
        if((typeof(loadState) != "undefined") && !loadState){
            if(jQuery.browser.msie){
                ue.tag("msie-img-not-loaded");
            } else if(jQuery.browser.mozilla) {
                ue.tag("mozilla-img-not-loaded");
            } else if(jQuery.browser.safari) {
                ue.tag("safari-img-not-loaded");
            } else if(jQuery.browser.webkit) {
                ue.tag("webkit-img-not-loaded");
            } else {
                ue.tag("unknown-img-not-loaded");
            }
        }
    }
})();
}

});
</script>







<script type='text/javascript'>
