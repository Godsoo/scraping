<script>
amznJQ.available('jQuery', function() {
    var $ = jQuery;
    (function(){
        $('#acsux-menu li:first').addClass('acsux-active');
          var appendString = '';
        appendString += "<li style='background:transparent url(http://g-ecx.images-amazon.com/images/G/01/img14/kitchen/slideshow/breakfast_sandwich_maker_slideshow._V341335260_.jpg) no-repeat center top; vertical-align;'><a href='/Hamilton-Beach-25475-Breakfast-Sandwich/dp/B00C95O3HK&ref_=acs_ux_hsb_5s_2_s_Kitchen_SA_SA-SMAPP' ><span class='hide-text'>Breakfast Made Easy</span></a></li>";
        appendString += "<li style='background:transparent url(http://g-ecx.images-amazon.com/images/G/01/img14/kitchen/slideshow/kit_hap_770x360_slideshow._V340914222_.jpg) no-repeat center top; vertical-align;'><a href='/s/?rh=i%3Akitchen%2Cn%3A1055398%2Cn%3A%211063498%2Cn%3A284507%2Cn%3A289913%2Cn%3A289941%2Cn%3A1090764%2Cp_n_feature_keywords_browse-bin%3A5024550011&bbn=1090764&ie=UTF8&ref_=acs_ux_hsb_5s_3_s_Kitchen_SA_SA-SMAPP' ><span class='hide-text'>Hot Air Poppers</span></a></li>";
        appendString += "<li style='background:transparent url(http://g-ecx.images-amazon.com/images/G/01/img13/kitchen-housewares/billboard/slideshow/hw_specialty_nesco-dehydrator_ss_770x340._V358377758_.jpg) no-repeat center top; vertical-align;'><a href='/Nesco-FD-75A-600-Watt-Food-Dehydrator/dp/B0090WOCN0&ref_=acs_ux_hsb_5s_4_s_Kitchen_SA_SA-SMAPP' ><span class='hide-text'>Nesco Dehydrators</span></a></li>";
        appendString += "<li style='background:transparent url(http://g-ecx.images-amazon.com/images/G/01/img13/kitchen-housewares/billboard/slideshow/hw_specialty_oster-auto-wine_ss_770x340._V358377753_.jpg) no-repeat center top; vertical-align;'><a href='/Oster-FPSTBW8207-S-Electric-Bottle-Opener/dp/B003YFI0O6&ref_=acs_ux_hsb_5s_5_s_Kitchen_SA_SA-SMAPP' ><span class='hide-text'>Wine with Oster</span></a></li>";

        $('#acsux-hero').append(appendString);

        acsuxSlider = {
            identifier: 'acs_ux_hsb_5s_',
            numSlides: 5,
            currSlide: 1,
            slidePos:0,
            animSpeed: 500,
            startSlide : setInterval(function(){
                    acsuxSlider.autoSlide();
            }, 5000),
            stopSlide: function(){
                clearInterval(acsuxSlider.startSlide);
            },
            slide : function(target, pos, type){
                if(type != 'a'){
                 acsuxSlider.emitEvent(acsuxSlider.identifier + target + '_0_0_' + type);
                }
                $('#acsux-menu').find('li').removeClass('acsux-active');
                $('#acsux-slide-'+target).addClass('acsux-active');
                $('#acsux-hero-wrap').animate({
                        top: '-'+pos
                    }, acsuxSlider.animSpeed);

                acsuxSlider.currSlide = target;
                acsuxSlider.slidePos = pos;
            
            },
            autoSlide: function(){
                if(acsuxSlider.currSlide < acsuxSlider.numSlides){
                    acsuxSlider.currSlide++;
                    acsuxSlider.slidePos += 360;
                    acsuxSlider.slide(acsuxSlider.currSlide, acsuxSlider.slidePos, 'a');
                } else {
                    acsuxSlider.slide(1, 0, 'a');
                    acsuxSlider.currSlide = 1;
                }
            },
            emitEvent: function(id){
                if(window.ue) {
                    ue.log({'v':1,'r':id}, 'acsux-widgets');     
                }
            }
        };

        //emit load events for widget and first slide
        acsuxSlider.emitEvent(acsuxSlider.identifier + '0_0_0_a');
        acsuxSlider.emitEvent(acsuxSlider.identifier + '1_0_0_a');
        
        $('#acsux-hero').find('a').hover(function(){
              clearInterval(acsuxSlider.startSlide);
        },
        function(){
                acsuxSlider.startSlide = setInterval(function(){
                    acsuxSlider.autoSlide();
            }, 5000);
        });
        $('#acsux-menu').find('li').hover(function(){
            var slideTo = parseInt($(this).attr('data-id'));
            var slidePos = parseInt($(this).attr('data-pos'));
            $('#acsux-menu').find('li').removeClass('acsux-active');
            $('#acsux-slide-'+slideTo).addClass('acsux-active');
            var t = setTimeout(function(){
                acsuxSlider.slide(slideTo, slidePos, 'u');
            }, acsuxSlider.animSpeed);
            $(this).data('timeout', t);
            clearInterval(acsuxSlider.startSlide);
        }, 
        function(){
            clearTimeout($(this).data('timeout'));
            acsuxSlider.startSlide = setInterval(function(){
                    acsuxSlider.autoSlide();
            }, 5000);
        });
    })()
});
            
</script>





